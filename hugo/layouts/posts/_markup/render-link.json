{{ if strings.HasPrefix .Destination "http" }}
  <a href="{{ .Destination }}" {{ with .Title }}title="{{ . }}"{{ end }} target="_blank"><span>{{ .Text }}</span><span className="ex-link" /></a>
{{ else }}
  <A prefetch={false} href="{{ .Destination }}" {{ with .Title }}title="{{ . }}"{{ end }}>{{ .Text }}</A>
{{ end }}
