{
  "name": "wicer-blog",
  "type": "module",
  "scripts": {
    "content:build": "velite build --clean",
    "dev": " vinxi dev",
    "build": "pnpm content:build && vinxi build --preset static",
    "start": "npx serve .output/public",
    "version": "vinxi version",
    "clean": "rm -rf .velite node_modules/.vinxi src/routes/\\(hugo\\) \\(hugo\\) .output public/_data public/sitemap.xml public/atom.xml public/manifest.webmanifest public/posts public/base public/category public/tags public/sass",
    "i18n": "typesafe-i18n"
  },
  "dependencies": {
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "^0.15.3",
    "@solidjs/start": "^1.1.4",
    "solid-js": "^1.9.6",
    "typesafe-i18n": "^5.26.2",
    "vinxi": "^0.5.6"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@iconify-json/ph": "^1.2.2",
    "@iconify-json/tabler": "^1.2.17",
    "@shikijs/rehype": "3.20.0",
    "@unocss/preset-typography": "^66.5.11",
    "@unocss/preset-wind3": "^66.5.11",
    "@unocss/reset": "^66.1.2",
    "katex": "^0.16.22",
    "qrcode": "^1.5.4",
    "rehype-katex": "^7.0.1",
    "rehype-slug": "^6.0.0",
    "remark-math": "^6.0.0",
    "tsx": "^4.19.2",
    "unocss": "^66.1.2",
    "unplugin-icons": "^22.1.0",
    "velite": "^0.3.1",
    "vite": "^6.3.5",
    "workbox-core": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "yaml": "^2.8.2"
  },
  "pnpm": {
    "patchedDependencies": {
      "vinxi": "patches/vinxi.patch",
      "@solidjs/start": "patches/@solidjs__start.patch"
    }
  }
}
